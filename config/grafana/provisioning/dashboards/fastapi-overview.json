{
  "id": null,
  "title": "FastAPI Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-1h", "to": "now" },
  "templating": {
    "list": [
      {
        "type": "query",
        "name": "job",
        "datasource": { "type": "prometheus", "uid": "Prometheus" },
        "query": "label_values(http_requests_total, job)",
        "refresh": 1,
        "current": { "text": "fastapi", "value": "fastapi" }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Requests/sec",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"$job\"}[1m]))",
          "legendFormat": "rps"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate (5xx)",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\",job=\"$job\"}[5m])) / sum(rate(http_requests_total{job=\"$job\"}[5m]))",
          "legendFormat": "error_rate"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95",
      "gridPos": { "x": 0, "y": 6, "w": 16, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"$job\"}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Status",
      "gridPos": { "x": 0, "y": 14, "w": 16, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"$job\"}[1m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ]
    }
  ]
}